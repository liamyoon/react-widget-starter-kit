{
  "name": "my-custom-widget",
  "version": "0.1.0",
  "description": "Example of using a React Widget in a Jupyterlab extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/liamyoon/react-widget-starter-kit",
  "bugs": {
    "url": "https://github.com/liamyoon/react-widget-starter-kit/issues"
  },
  "license": "BSD-3-Clause",
  "author": "",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/liamyoon/react-widget-starter-kit"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "link": "jupyter labextension link . --no-build",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/completer": "^3.0.0",
    "@jupyterlab/hub-extension": "^3.0.0",
    "@jupyterlab/launcher": "^3.0.0",
    "@jupyterlab/notebook": "^3.0.0",
    "@jupyterlab/ui-components": "^3.0.0",
    "axios": "^0.21.1",
    "react-modal": "^3.12.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "rimraf": "^3.0.0",
    "typescript": "~3.7.5"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
